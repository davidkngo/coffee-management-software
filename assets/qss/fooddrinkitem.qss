FDItemWidget {
    background: #ffffff;
    border-radius: 15px;
}

FDItemWidget > QPushButton {
    background: aqua;
    border-radius: 12px;
    font-size: 10pt;
    font-weight: bold;
    color: whitesmoke;

}

FDItemImage {
    background: transparent;
}

FDItemWidget > QPushButton:hover:!pressed {
    background: aquamarine;
}

FDItemWidget > QLabel {
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    font-size: 14pt;
}

