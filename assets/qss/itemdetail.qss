QPushButton {
    background-color: #4CAF50; /* Green */
    border: none;
    color: white;
    padding: 15px 32px;
    text-align: center;
    text-decoration: none;
    font-size: 16px;
    width: 50px;
    height: 20px;
}

QLabel#imageThumbnail {
    border: 1px solid black;
}

QPushButton#browseBtn {
    background: rgba(0, 0, 0, 0.1);
    color: white;
    height: 30px;
}

QPushButton#browseBtn:hover {
    background: rgba(0, 0, 0, 0.2);
}